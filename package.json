{
  "name": "swtle",
  "version": "1.0.0",
  "description": "",
  "main": "client.js",
  "type": "module",
  "scripts": {
    "start": "npm run server:build && node server/build",
    "server:client": "nodemon client.js",
    "server:admin": "nodemon admin.js",
    "client": "npm run dev --prefix client",
    "client:admin": "npm start --prefix admin",
    "admin:dev": "concurrently \"npm run client:admin\"  \"npm run server:admin\"",
    "client:dev": "concurrently \"npm run client\"  \"npm run server:client\"",
    "client:build": "npm run build --prefix client",
    "admin:build": "npm run build --prefix admin",
    "server:build": "npm run server:client:build && npm run server:admin:build",
    "server:client:build": "esbuild client.js --packages=external --bundle --minify --platform=node --target=node18 --outfile=dist/client.js",
    "server:admin:build": "esbuild admin.js --packages=external --bundle --minify --platform=node --target=node18 --outfile=dist/admin.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rahman-a/swtle.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rahman-a/swtle/issues"
  },
  "homepage": "https://github.com/rahman-a/swtle#readme",
  "dependencies": {
    "@types/mongoose": "^5.11.97",
    "axios": "^0.21.4",
    "bcrypt": "^5.0.1",
    "bson-objectid": "^2.0.3",
    "chalk": "^2.4.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^12.0.4",
    "express": "^4.17.2",
    "full-icu": "^1.4.0",
    "handlebars": "^4.7.7",
    "helmet": "^5.0.1",
    "i18next": "^21.6.13",
    "i18next-fs-backend": "^1.1.4",
    "i18next-http-middleware": "^3.2.0",
    "jsonwebtoken": "^8.5.1",
    "luxon": "^1.28.0",
    "mailgun-js": "^0.6.7",
    "mongoose": "^6.1.6",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "node-cron": "^3.0.0",
    "node-schedule": "^2.1.0",
    "puppeteer": "^19.7.2",
